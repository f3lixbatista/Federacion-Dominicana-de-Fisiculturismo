<%- include("template/cabecera", {tituloWeb: "Pagina de Atletas"}) %>

<div class="container">

    <table class="table">
        <thead>
          <tr>
            <th scope="col">ID FDFF</th>
            <th scope="col">Nombre</th>
            <th scope="col">Categoria</th>
            <th scope="col">Accion</th>
          </tr>
        </thead>
        <tbody>

            <% if (arrayAtletas.length > 0) { %>
                <% arrayAtletas.forEach(atletas => { %>
                    <tr>
                        <th scope="row"><%= atletas.IDFDFF %></th>
                        <td><%= atletas.Nombre %></td>
                        <td><%= atletas.Categoria %></td>
                        <td>@mdo</td>
                    </tr>
                <% }) %>
             <% } %>


        </tbody>
      </table>


  




